{
  "name": "xunlongheAdmin",
  "version": "0.2.0",
  "description": "浔龙河后台综合管理系统. 基于antd-pro 开发;继承dva;dva 技术栈其实就是react,react-router,redux,react-router-redux,react-redux,redux-saga的集成",
  "private": true,
  "scripts": {
    "precommit": "npm run lint-staged",
    "start": "cross-env DISABLE_ESLINT=true roadhog dev",
    "start:no-proxy": "cross-env NO_PROXY=true DISABLE_ESLINT=true roadhog dev",
    "build": "cross-env DISABLE_ESLINT=true roadhog build",
    "site": "roadhog-api-doc static && gh-pages -d dist",
    "analyze": "ANALYZE=true roadhog build",
    "lint:style": "stylelint \"src/**/*.less\" --syntax less",
    "lint-staged": "lint-staged",
    "lint-staged:js": "eslint --ext .js",
    "test": "roadhog test",
    "test:component": "roadhog test ./src/components",
    "test:all": "node ./tests/run-tests.js"
  },
  "dependencies": {
    "@babel/polyfill": "7.0.0-beta.40",
    "antd": "3.3.0",
    "axios": "0.18.0",
    "babel-runtime": "6.26.0",
    "classnames": "^2.2.5",
    "dva": "^2.1.0",
    "echarts": "^3.8.5",
    "file-saver": "^1.3.3",
    "lodash": "^4.17.4",
    "lodash-decorators": "^4.4.1",
    "lodash.clonedeep": "^4.5.0",
    "moment": "2.21.0",
    "numeral": "2.0.6",
    "path-to-regexp": "2.2.0",
    "prop-types": "15.6.1",
    "qrcode.react": "0.8.0",
    "qs": "6.5.1",
    "react": "16.2.0",
    "react-amap": "1.2.3",
    "react-container-query": "0.11.0",
    "react-document-title": "2.0.3",
    "react-dom": "16.2.0",
    "react-fittext": "1.0.0",
    "script-loader": "0.7.2",
    "url-polyfill": "1.0.11",
    "workbook": "1.1.3",
    "xlsx": "0.12.5"
  },
  "devDependencies": {
    "babel-eslint": "8.2.2",
    "babel-plugin-dva-hmr": "^0.4.1",
    "babel-plugin-import": "1.6.6",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "cross-env": "^5.1.1",
    "cross-port-killer": "^1.0.1",
    "enzyme": "3.3.0",
    "eslint": "^4.16.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-babel": "4.1.2",
    "eslint-plugin-compat": "2.2.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-markdown": "^1.0.0-beta.6",
    "eslint-plugin-react": "7.6.1",
    "gh-pages": "1.1.0",
    "husky": "^0.14.3",
    "lint-staged": "^6.1.0",
    "pro-download": "^1.0.1",
    "redbox-react": "^1.5.0",
    "regenerator-runtime": "^0.11.1",
    "roadhog": "2.2.0",
    "roadhog-api-doc": "1.0.0",
    "rollbar": "2.3.8",
    "stylelint": "^8.4.0",
    "stylelint-config-standard": "^18.0.0"
  },
  "optionalDependencies": {
    "nightmare": "^2.10.0"
  },
  "lint-staged": {
    "**/*.{js,jsx}": "lint-staged:js",
    "**/*.less": "stylelint --syntax less"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ]
}
